<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="user" meta_type="CMF Action Category">
  <object name="SetupPassword" meta_type="CMF Action" i18n:domain="tpa.content" insert-before="logout">
   <property name="title" i18n:translate="">Setup Password</property>
   <property name="description" i18n:translate="">Setup password</property>
   <property name="url_expr">string:${portal_url}/@@setupPassword</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>

